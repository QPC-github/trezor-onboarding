{
  "name": "trezor-onboarding",
  "version": "1.0.0",
  "repository": "https://github.com/mroz22/trezor-onboarding",
  "author": "Monkey donkey",
  "license": "to be updated",
  "scripts": {
    "dev": "webpack-dev-server --env.BUILD=development",
    "lint": "run-s lint:*",
    "lint:js": "npx eslint ./src",
    "lint:css": "npx stylelint './src/**/*.js'",
    "build:beta": "rimraf dist && npx webpack --progress --bail --env.BUILD=beta",
    "build:production": "rimraf dist && npx webpack --progress --bail --env.BUILD=production",
    "test-all": "jest",
    "test-unit": "jest --testPathPattern=src",
    "test-health": "jest --testPathPattern=tests/health",
    "translations:extract": "rimraf translations/extractedMessages && cross-env NODE_ENV=translations babel src",
    "translations:upload": "yarn translations:extract && yarn ttm merge-msgs && yarn ttm build-csv && yarn ttm upload",
    "translations:download": "yarn ttm build-translations && sleep 1 && yarn ttm export-translations"
  },
  "dependencies": {
    "@dragonraider5/react-intl": "^2.7.0",
    "@sentry/browser": "^4.6.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "connected-react-router": "^6.3.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-intl": "^2.8.0",
    "react-redux": "^6.0.0",
    "react-select": "^2.4.2",
    "react-svg": "^7.2.6",
    "react-timeout": "^1.2.0",
    "react-transition-group": "^2.9.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.3",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.4",
    "styled-reset": "^1.6.4",
    "trezor-connect": "../connect/npm/",
    "trezor-flags": "^1.0.2",
    "trezor-ui-components": "^1.0.0-beta.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-react-intl": "^3.0.1",
    "cross-env": "^5.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "node-fetch": "^2.3.0",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-processor-styled-components": "^1.5.1",
    "svg-inline-loader": "^0.8.0",
    "trezor-translations-manager": "^1.0.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
